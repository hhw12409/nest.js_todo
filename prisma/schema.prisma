// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// Todo 모델
model Todo {
  id    Int     @default(autoincrement()) @id
  title String  
  content  String?
  is_done Boolean? @default(false)
}